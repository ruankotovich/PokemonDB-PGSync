{
  "name": "pkm_db",
  "description": "pkm_db",
  "version": "0.0.38",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build --force",
    "start": "NODE_ENV=production node dist/src/index",
    "start:dev": "TS_NODE_FILES=true ts-node-dev --ignore-watch node_modules --project tsconfig.json src/index.ts",
    "pretest": "tsc --build --force",
    "test": "NODE_ENV=test npm run db-sync && jest --config jest.config.json",
    "test:unit": "jest --coverage --config jest.config.json",
    "test:watch": "jest --watch --config jest.config.json",
    "db-sync": "ts-node scripts/db-sync.ts",
    "dry-run": "ts-node scripts/dry-run.ts",
    "lint": "eslint src/** test/** --ext ts",
    "lint:fix": "eslint src/** test/** --ext ts --fix",
    "sequelize-cli": "node_modules/.bin/sequelize-cli"
  },
  "devDependencies": {
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.168",
    "@types/pg": "^7.14.10",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "ts-jest": "^26.1.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.11.0",
    "@elastic/elasticsearch-mock": "^0.3.0",
    "@types/bluebird": "^3.5.32",
    "@types/node": "^14.10.2",
    "@types/validator": "^13.1.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.14.0",
    "date-fns-timezone": "^0.1.4",
    "express": "^4.17.1",
    "http-status": "^1.5.0",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.0",
    "nock": "^13.0.9",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  }
}
